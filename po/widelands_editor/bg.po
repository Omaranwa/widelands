msgid "Toggle tools menu"
msgstr "Превключване на менюто с инструментите"

#. TRANSLATORS: This is an access key combination. Localize, but do not change
#. the key.
#: ../../data/scripting/editor/editor_help.lua:25
msgctxt "hotkey"
msgid "1-0"
msgstr "1-0"

#: ../../data/scripting/editor/editor_help.lua:25
msgid "Change tool size"
msgstr "Промяна на размера на инструмента"

#. TRANSLATORS: This is an access key combination. The hotkey is 'p'
#: ../../data/scripting/editor/editor_help.lua:29
msgid "Toggle player menu"
msgstr "Превключване на менюто за играча"

#. TRANSLATORS: This is an access key combination. Localize, but do not change
#. the key.
