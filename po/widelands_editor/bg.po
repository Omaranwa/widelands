#. the key.
#: ../../data/scripting/editor/editor_help.lua:35
msgid "Help"
msgstr "Помощ"

#: ../../data/scripting/editor/editor_help.lua:37
msgid "First alternative tool while pressed"
msgstr "Избиране на първия алтернативен инструмент, докато е задържан"

#. TRANSLATORS: This is an access key combination. Localize, but do not change
#. the key.
#: ../../data/scripting/editor/editor_help.lua:37
msgctxt "hotkey"
msgid "Shift (Hold)"
msgstr "Шифт (задържан)"

#. TRANSLATORS: This is an access key combination. Localize, but do not change
#. the key.
#: ../../data/scripting/editor/editor_help.lua:39
msgctxt "hotkey"
msgid "Alt (Hold)"
msgstr "Алт (задържан)"

#: ../../data/scripting/editor/editor_help.lua:39
msgid "Second alternative tool while pressed"
msgstr "Избиране на втория алтернативен инструмент, докато е задържан"

#. TRANSLATORS: This is an access key combination. The hotkey is 'i'
#: ../../data/scripting/editor/editor_help.lua:41
msgid "Activate information tool"
msgstr "Включване на инструмента за информация"

#. TRANSLATORS: This is an access key combination. Localize, but do not change
#. the key.
#: ../../data/scripting/editor/editor_help.lua:43
msgctxt "hotkey"
msgid "Ctrl + L"
msgstr "Контрол + L"

#: ../../data/scripting/editor/editor_help.lua:43
msgid "Load map"
msgstr "Зареждане на карта"

#. TRANSLATORS: This is an access key combination. Localize, but do not change
#. the key.
#: ../../data/scripting/editor/editor_help.lua:45
msgctxt "hotkey"
